<h1>quick_pick a password</h1>

<form action="/password_pages">
  <div>
    <label for="c_num">number of characters</label><br />
    <input id="c_num" name="c_num" type="text" />
  </div>
  <div>
    <label for="security">how secure?</label><br />
    <select>
      <option value="strong">Strong (8+ characters)</option>
      <option value="medium">Medium (4+ characters)</option>
      <option value="weak">Weak (less than 4 characters)</option>
    </select>
  </div>
  <div>
    <input type="submit">
  </div>
</form>

